<?xml version="1.0" encoding="UTF-8"?>
<registry>
    <portlet-entry name="WebMailAccountAdmin" hidden="false" type="ref" parent="Velocity" application="false">
        <security-ref parent="admin-view"/>
        <meta-info>
            <title>Webメール</title>
            <description>設定したアカウントのメールが送受信できるWebメール機能です。オリジナルの署名も設定することができます。</description>
        </meta-info>
        <classname>org.apache.jetspeed.portal.portlets.VelocityPortlet</classname>
        <parameter name="template" value="webmail-account-admin" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <parameter name="action" value="webmail.WebMailAdminAction" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <parameter name="_showtitlebar" value="false" hidden="true" cachedOnName="true" cachedOnValue="true"/>
        <media-type ref="html"/>
        <url cachedOnURL="true"/>
    </portlet-entry>
</registry>
